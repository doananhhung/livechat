# Database Configuration
PSQL_HOST=localhost
PSQL_PORT=5433
PSQL_USER=your_db_user
PSQL_PASSWORD=your_db_password
PSQL_DATABASE=your_db_name

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379

# JWT Configuration
JWT_SECRET=supersecretjwtkey
JWT_EXPIRES_IN=7d
JWT_REFRESH_SECRET=supersecretjwtrefreshkey
JWT_REFRESH_EXPIRES_IN=30d
TWO_FACTOR_AUTH_JWT_SECRET=supersecret2fajwtkey
TWO_FACTOR_AUTH_JWT_EXPIRES_IN=5m
TWO_FACTOR_APP_NAME=YourAppName

# API Configuration
API_BASE_URL=http://localhost:3000/api/v1
NODE_ENV=development

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# Encryption Key
ENCRYPTION_KEY=your_32_char_encryption_key

# Frontend URLs
FRONTEND_URL=http://localhost:5173
FRONTEND_AUTH_CALLBACK_URL=http://localhost:5173/auth/callback
FRONTEND_2FA_URL=http://localhost:5173/2fa

# AWS Configuration (for LocalStack in development)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=test
AWS_SECRET_ACCESS_KEY=test
AWS_SQS_ENDPOINT=http://localhost:4566
AWS_SQS_QUEUE_URL=http://localhost:4566/000000000000/your-queue-name
AWS_SQS_QUEUE_NAME=your-queue-name

# Email Configuration
MAIL_APP_PASSWORD=your_email_app_password
MAIL_USER=your_email@example.com
